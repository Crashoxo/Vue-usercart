O,CAAC,GAAR,CAAY,WAAZ,EAAyB,QAAzB;AACA,QAAA,KAAI,CAAC,SAAL,GAAiB,KAAjB;AACD,OAJD;AAKD,KAVM;AAWP,IAAA,UAXO,sBAWI,EAXJ,EAWQ;AACb;AACA,WAAK,OAAL,CAAa,IAAb,yBAAmC,EAAnC;AACD,KAdM;AAeP,IAAA,OAfO,mBAeC,EAfD,EAeK;AAAA;;AACV;AACA;AACA,WAAK,MAAL,CAAY,WAAZ,GAA0B,EAA1B,CAHU,CAGoB;;AAC9B,MAAA,OAAO,CAAC,GAAR,CAAY,IAAZ,EAAkB,EAAlB;AACA,UAAM,GAAE,aAAO,OAAO,CAAC,GAAR,CAAY,WAAnB,iBAAqC,OAAO,CAAC,GAAR,CAAY,YAAjD,UAAR;AACA,UAAM,IAAG,GAAI;AACX,QAAA,UAAU,EAAE,EADD;AAEX,QAAA,GAAG,EAAE;AAFM,OAAb;AAIA,WAAK,KAAL,CAAW,IAAX,CAAgB,GAAhB,EAAqB;AAAE,QAAA,IAAI,EAAE;AAAR,OAArB,EAAqC,IAArC,CAA0C,UAAC,GAAD,EAAS;AACjD,QAAA,MAAI,CAAC,MAAL,CAAY,WAAZ,GAA0B,EAA1B,CADiD,CACnB;;AAC9B,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAmB,GAAnB,EAFiD,CAGjD;;AACA,QAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,YAAlB;AACD,OALD;AAMD,KA/BM;AAgCP,IAAA,WAhCO,uBAgCK,EAhCL,EAgCS;AACd,MAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,MAAA,EAAE,CAAC,KAAH,CAAS,OAAT,GAAmB,CAAnB;AACA,MAAA,EAAE,CAAC,KAAH,CAAS,SAAT,GAAqB,kBAArB;AACD,KApCM;AAqCP,IAAA,KArCO,iBAqCD,EArCC,EAqCG;AACR,MAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,MAAA,IAAI,CAAC,cAAL,CAAoB,aAApB;AACA,MAAA,IAAI,CAAC,EAAL,CAAQ,EAAR,EAAY;AACV,QAAA,OAAO,EAAE,CADC;AAEV;AACA,QAAA,CAAC,EAAE,CAHO;AAIV,QAAA,QAAQ,EAAE,CAJA;AAKV,QAAA,KAAK,EAAE,EAAE,CAAC,OAAH,CAAW,KAAX,GAAmB,GALhB;AAMV,QAAA,aAAa,EAAE;AACb,UAAA,OAAO,EAAE,EADI;AAEb;AACA,UAAA,aAAa,EAAE,qBAHF;AAIb;AACA,UAAA,KAAK,EAAE,YALM,CAMb;AACA;;AAPa;AANL,OAAZ;AAgBD,KAxDM;AAyDP,IAAA,iBAzDO,6BAyDW,EAzDX,EAyDe;AACpB,MAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,MAAA,EAAE,CAAC,KAAH,CAAS,OAAT,GAAmB,CAAnB;AACA,MAAA,EAAE,CAAC,KAAH,CAAS,SAAT,GAAqB,kBAArB;AACD,KA7DM;AA8DP,IAAA,WA9DO,uBA8DK,EA9DL,EA8DS;AACd,MAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACA,MAAA,IAAI,CAAC,cAAL,CAAoB,aAApB;AACA,MAAA,IAAI,CAAC,EAAL,CAAQ,EAAR,EAAY;AACV,QAAA,OAAO,EAAE,CADC;AAEV,QAAA,CAAC,EAAE,CAFO;AAGV,QAAA,CAAC,EAAE,CAHO;AAIV,QAAA,QAAQ,EAAE,CAJA;AAKV,QAAA,KAAK,EAAE,EAAE,CAAC,OAAH,CAAW,KAAX,GAAmB,GALhB;AAMV,QAAA,aAAa,EAAE;AACb,UAAA,OAAO,EAAE,EADI;AAEb;AACA,UAAA,aAAa,EAAE,qBAHF;AAIb;AACA,UAAA,KAAK,EAAE,YALM,CAMb;AACA;;AAPa;AANL,OAAZ;AAgBD;AAjFM,GApBI;AAuGb;AACA,EAAA,OAxGa,qBAwGH;AACR,SAAK,WAAL;AACD,GA1GY;AA2Gb,EAAA,OA3Ga,qBA2GH;AACR;AACA,IAAA,IAAI,CAAC,cAAL,CAAoB,aAApB;AACA,IAAA,IAAI,CAAC,EAAL,CAAQ,QAAR,EAAkB;AAChB,MAAA,OAAO,EAAE,CADO;AAEhB,MAAA,QAAQ,EAAE,CAFM;AAGhB,MAAA,CAAC,EAAE,CAHa;AAIhB,MAAA,CAAC,EAAE,GAJa;AAKhB,MAAA,aAAa,EAAE;AACb,QAAA,OAAO,EAAE,QADI;AAEb;AACA,QAAA,aAAa,EAAE,qBAHF;AAIb,QAAA,KAAK,EAAE,IAJM;AAIA;AACb,QAAA,KAAK,EAAE,KALM,CAMb;AACA;;AAPa;AALC,KAAlB;AAeA,IAAA,IAAI,CAAC,EAAL,CAAQ,SAAR,EAAmB;AACjB,MAAA,OAAO,EAAE,CADQ;AAEjB,MAAA,QAAQ,EAAE,CAFO;AAGjB,MAAA,CAAC,EAAE,CAHc;AAIjB,MAAA,CAAC,EAAE,GAJc;AAKjB,MAAA,aAAa,EAAE;AACb,QAAA,OAAO,EAAE,SADI;AAEb;AACA,QAAA,aAAa,EAAE,qBAHF;AAIb,QAAA,KAAK,EAAE,IAJM;AAIA;AACb,QAAA,KAAK,EAAE,KALM,CAMb;AACA;;AAPa;AALE,KAAnB;AAeD;AA5IY,CAAf","sourcesContent":["<template>\r\n  <!-- 產品總頁面/購物車 -->\r\n  <div>\r\n    <Loading :active=\"isLoading\"></Loading>\r\n    <div\r\n      id=\"carouselExampleCaptions\"\r\n      class=\"carousel slide\"\r\n      data-bs-ride=\"carousel\"\r\n    >\r\n      <div class=\"carousel-indicators\">\r\n        <button\r\n          type=\"button\"\r\n          data-bs-target=\"#carouselExampleCaptions\"\r\n          data-bs-slide-to=\"0\"\r\n          class=\"active\"\r\n          aria-current=\"true\"\r\n          aria-label=\"Slide 1\"\r\n        ></button>\r\n        <button\r\n          type=\"button\"\r\n          data-bs-target=\"#carouselExampleCaptions\"\r\n          data-bs-slide-to=\"1\"\r\n          aria-label=\"Slide 2\"\r\n        ></button>\r\n        <button\r\n          type=\"button\"\r\n          data-bs-target=\"#carouselExampleCaptions\"\r\n          data-bs-slide-to=\"2\"\r\n          aria-label=\"Slide 3\"\r\n        ></button>\r\n      </div>\r\n      <div class=\"carousel-inner\">\r\n        <div class=\"carousel-item active\" data-bs-interval=\"3000\">\r\n          <img src=\"../assets/img/a01.jpg\" class=\"d-block w-100\" alt=\"...\" />\r\n          <div class=\"carousel-caption d-none d-md-block\">\r\n            <h5>First slide label</h5>\r\n            <p>\r\n              The greatest test of courage on earth is to bear defeat without\r\n              losing heart.\r\n            </p>\r\n          </div>\r\n        </div>\r\n        <div class=\"carousel-item\" data-bs-interval=\"3000\">\r\n          <img src=\"../assets/img/a03.jpg\" class=\"d-block w-100\" alt=\"...\" />\r\n          <div class=\"carousel-caption d-none d-md-block\">\r\n            <h5>Second slide label</h5>\r\n            <p>\r\n              Life is the art of drawing sufficient conclusions form\r\n              insufficient premises.\r\n            </p>\r\n          </div>\r\n        </div>\r\n        <div class=\"carousel-item\" data-bs-interval=\"3000\">\r\n          <img src=\"../assets/img/a02.jpg\" class=\"d-block w-100\" alt=\"...\" />\r\n          <div class=\"carousel-caption d-none d-md-block\">\r\n            <h5>Third slide label</h5>\r\n            <p>\r\n              There's only one corner of the universe you can be sure of\r\n              improving, and that's your own self.\r\n            </p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <button\r\n        class=\"carousel-control-prev\"\r\n        type=\"button\"\r\n        data-bs-target=\"#carouselExampleCaptions\"\r\n        data-bs-slide=\"prev\"\r\n      >\r\n        <span class=\"carousel-control-prev-icon\" aria-hidden=\"true\"></span>\r\n        <span class=\"visually-hidden\">Previous</span>\r\n      </button>\r\n      <button\r\n        class=\"carousel-control-next\"\r\n        type=\"button\"\r\n        data-bs-target=\"#carouselExampleCaptions\"\r\n        data-bs-slide=\"next\"\r\n      >\r\n        <span class=\"carousel-control-next-icon\" aria-hidden=\"true\"></span>\r\n        <span class=\"visually-hidden\">Next</span>\r\n      </button>\r\n    </div>\r\n    <div class=\"container\">\r\n      <!-- 卡片 -->\r\n      <h1 class=\"title\">可使用 管理者登入</h1>\r\n      <div class=\"row row-cols-1 row-cols-md-3 g-4\">\r\n        <!-- transition-group 下必須馬上接 v-for 才吃的到 key-->\r\n        <transition-group appear @before-enter=\"beforeEnter\" @enter=\"enter\">\r\n          <div\r\n            class=\"col\"\r\n            v-for=\"(item, key) in products\"\r\n            :data-index=\"key\"\r\n            :key=\"item.id + key\"\r\n          >\r\n            <div class=\"card product\">\r\n              <img\r\n                v-on:click.prevent=\"getProduct(item.id)\"\r\n                :src=\"`${item.imageUrl}`\"\r\n                class=\"card-img-top\"\r\n                alt=\"...\"\r\n              />\r\n              <div class=\"card-body\">\r\n                <h5 class=\"card-title text-center\">{{ item.title }}</h5>\r\n                <div class=\"card-text text-center\">\r\n                  <div class=\"h5\" v-if=\"!item.price\">\r\n                    {{ item.origin_price }} 元\r\n                  </div>\r\n                  <br />\r\n                  <del class=\"h6\" v-if=\"item.price\"\r\n                    >original price {{ item.origin_price }} NT</del\r\n                  >\r\n\r\n                  <div class=\"h5\" v-if=\"item.price\">\r\n                    special offer {{ $filters.currency(item.price) }} NT\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <button\r\n                @click=\"getProduct(item.id)\"\r\n                type=\"button\"\r\n                class=\"btn btn-outline-secondary\"\r\n                style=\"font-weight: bold\"\r\n              >\r\n                SEE MORE\r\n              </button>\r\n              <!-- API需要product_id(String)、qty(Number) -->\r\n              <!-- 樣式：disabled 按鈕不能按 -->\r\n              <!-- 如果倉庫收到的值 === 當前id -->\r\n              <button\r\n                v-on:click=\"addCart(item.id)\"\r\n                v-bind:disabled=\"this.status.loadingItem === item.id\"\r\n                type=\"button\"\r\n                class=\"btn btn-outline-danger\"\r\n                style=\"font-weight: bold\"\r\n              >\r\n                <!-- BS樣式 -->\r\n                <!-- https://getbootstrap.com/docs/5.1/components/spinners/ -->\r\n                <div\r\n                  v-if=\"this.status.loadingItem === item.id\"\r\n                  class=\"spinner-grow spinner-grow-sm text-danger\"\r\n                  role=\"status\"\r\n                >\r\n                  <span class=\"visually-hidden\">Loading...</span>\r\n                </div>\r\n                Add Cart\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </transition-group>\r\n      </div>\r\n\r\n      <h1 class=\"title2\">以下中文版產品列表</h1>\r\n\r\n      <!-- 產品列表 -->\r\n      <div class=\"row mt-4\">\r\n        <!-- \"col-md-7\" -->\r\n        <div class=\"\">\r\n          <table class=\"table align-middle\">\r\n            <thead>\r\n              <tr>\r\n                <th>圖片</th>\r\n                <th>商品名稱</th>\r\n                <th>價格</th>\r\n                <th></th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <!-- transition-group 下必須馬上接 v-for 才吃的到 key-->\r\n              <transition-group\r\n                appear\r\n                @before-enter=\"beforeEnterBottom\"\r\n                @enter=\"enterBottom\"\r\n              >\r\n                <tr\r\n                  v-for=\"(item, key) in products\"\r\n                  :data-index=\"key\"\r\n                  :key=\"item.id\"\r\n                >\r\n                  <td style=\"width: 200px\">\r\n                    <div\r\n                      style=\"\r\n                        height: 100px;\r\n                        background-size: cover;\r\n                        background-position: center;\r\n                      \"\r\n                      :style=\"{ backgroundImage: `url(${item.imageUrl})` }\"\r\n                    ></div>\r\n                  </td>\r\n                  <td>\r\n                    <a\r\n                      href=\"\"\r\n                      v-on:click.prevent=\"getProduct(item.id)\"\r\n                      class=\"btn-outline-light btn\"\r\n                      >{{ item.title }}</a\r\n                    >\r\n                  </td>\r\n                  <td>\r\n                    <div class=\"h5\" v-if=\"!item.price\">\r\n                      {{ item.origin_price }} 元\r\n                    </div>\r\n                    <del class=\"h6\" v-if=\"item.price\"\r\n                      >原價 {{ item.origin_price }} 元</del\r\n                    >\r\n                    <div class=\"h5\" v-if=\"item.price\">\r\n                      現在只要 {{ $filters.currency(item.price) }} 元\r\n                    </div>\r\n                  </td>\r\n                  <td>\r\n                    <div class=\"btn-group btn-group-sm\">\r\n                      <button\r\n                        type=\"button\"\r\n                        class=\"btn btn-outline-secondary\"\r\n                        @click=\"getProduct(item.id)\"\r\n                      >\r\n                        查看更多\r\n                      </button>\r\n                      <!-- API需要product_id(String)、qty(Number) -->\r\n                      <!-- 樣式：disabled 按鈕不能按 -->\r\n                      <!-- 如果倉庫收到的值 === 當前id -->\r\n                      <button\r\n                        v-on:click=\"addCart(item.id)\"\r\n                        v-bind:disabled=\"this.status.loadingItem === item.id\"\r\n                        type=\"button\"\r\n                        class=\"btn btn-outline-danger\"\r\n                      >\r\n                        <!-- BS樣式 -->\r\n                        <!-- https://getbootstrap.com/docs/5.1/components/spinners/ -->\r\n                        <div\r\n                          v-if=\"this.status.loadingItem === item.id\"\r\n                          class=\"spinner-grow spinner-grow-sm text-danger\"\r\n                          role=\"status\"\r\n                        >\r\n                          <span class=\"visually-hidden\">Loading...</span>\r\n                        </div>\r\n                        加到購物車\r\n                      </button>\r\n                    </div>\r\n                  </td>\r\n                </tr>\r\n              </transition-group>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div style=\"height: 400px\"></div>\r\n</template>\r\n\r\n<script>\r\nimport gsap from \"gsap\";\r\nimport { ScrollTrigger } from \"gsap/dist/ScrollTrigger\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [],\r\n      product: {},\r\n      status: {\r\n        // 小小讀取效果 + 購物車按鈕暫時無法執行 倉庫\r\n        loadingItem: \"\", //對應品項id\r\n      },\r\n      cart: {},\r\n      // 供轉址使用\r\n      orderId: \"\",\r\n      // 分頁資訊\r\n      pagination: {},\r\n      // 判斷是否要loading轉圈圈 倉庫\r\n      isLoading: false,\r\n    };\r\n  },\r\n  // 引用emitter (因 Dashboard.vue provide )\r\n  inject: [\"emitter\"],\r\n  methods: {\r\n    getProducts() {\r\n      // 商品 啟用 才會顯示在前台\r\n      const url = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/products/all`;\r\n      this.isLoading = true;\r\n      this.$http.get(url).then((response) => {\r\n        this.products = response.data.products;\r\n        console.log(\"products:\", response);\r\n        this.isLoading = false;\r\n      });\r\n    },\r\n    getProduct(id) {\r\n      // 推給 index.js網址列\r\n      this.$router.push(`/user/product/${id}`);\r\n    },\r\n    addCart(id) {\r\n      // 新增購物車\r\n      // https://github.com/hexschool/vue3-course-api-wiki/wiki/%E5%AE%A2%E6%88%B6%E8%B3%BC%E7%89%A9-%5B%E5%85%8D%E9%A9%97%E8%AD%89%5D#%E5%8A%A0%E5%85%A5%E8%B3%BC%E7%89%A9%E8%BB%8A\r\n      this.status.loadingItem = id; // 按下按鈕送id，賦值給倉庫\r\n      console.log(\"id\", id);\r\n      const url = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/cart`;\r\n      const cart = {\r\n        product_id: id,\r\n        qty: 1,\r\n      };\r\n      this.$http.post(url, { data: cart }).then((res) => {\r\n        this.status.loadingItem = \"\"; //資料庫更新成功後，空\r\n        console.log(\"res\", res);\r\n        // 轉跳登入頁面\r\n        this.$router.push(\"/user/cart\");\r\n      });\r\n    },\r\n    beforeEnter(el) {\r\n      console.log(\"aaa\");\r\n      el.style.opacity = 0;\r\n      el.style.transform = \"translateY(60px)\";\r\n    },\r\n    enter(el) {\r\n      console.log(\"aaa\");\r\n      gsap.registerPlugin(ScrollTrigger);\r\n      gsap.to(el, {\r\n        opacity: 1,\r\n        // x: 100,\r\n        y: 0,\r\n        duration: 1,\r\n        delay: el.dataset.index * 0.2,\r\n        scrollTrigger: {\r\n          trigger: el,\r\n          // 在這個情境 trigger 要是物件，不是選擇器\r\n          toggleActions: \"play none none none\",\r\n          // scrub: true, // 物件動畫根據卷軸捲動程度跑\r\n          start: \"top center\",\r\n          // end: \"+=100\",\r\n          // markers: true,\r\n        },\r\n      });\r\n    },\r\n    beforeEnterBottom(el) {\r\n      console.log(\"aaa\");\r\n      el.style.opacity = 0;\r\n      el.style.transform = \"translateY(60px)\";\r\n    },\r\n    enterBottom(el) {\r\n      console.log(\"aaa\");\r\n      gsap.registerPlugin(ScrollTrigger);\r\n      gsap.to(el, {\r\n        opacity: 1,\r\n        y: 0,\r\n        x: 0,\r\n        duration: 1,\r\n        delay: el.dataset.index * 0.5,\r\n        scrollTrigger: {\r\n          trigger: el,\r\n          // 在這個情境 trigger 要是物件，不是選擇器\r\n          toggleActions: \"play none none none\",\r\n          // scrub: true, // 物件動畫根據卷軸捲動程度跑\r\n          start: \"top center\",\r\n          // end: \"+=100\",\r\n          // markers: true,\r\n        },\r\n      });\r\n    },\r\n  },\r\n  // 生命週期，網頁開始就跑\r\n  created() {\r\n    this.getProducts();\r\n  },\r\n  mounted() {\r\n    // title動畫\r\n    gsap.registerPlugin(ScrollTrigger);\r\n    gsap.to(\".title\", {\r\n      opacity: 0,\r\n      duration: 1,\r\n      y: 0,\r\n      x: 100,\r\n      scrollTrigger: {\r\n        trigger: \".title\",\r\n        // 在這個情境 trigger 要是物件，不是選擇器\r\n        toggleActions: \"play none none none\",\r\n        scrub: true, // 物件動畫根據卷軸捲動程度跑\r\n        start: \"top\",\r\n        // end: \"+=100\",\r\n        // markers: true,\r\n      },\r\n    });\r\n    gsap.to(\".title2\", {\r\n      opacity: 0,\r\n      duration: 1,\r\n      y: 0,\r\n      x: 100,\r\n      scrollTrigger: {\r\n        trigger: \".title2\",\r\n        // 在這個情境 trigger 要是物件，不是選擇器\r\n        toggleActions: \"play none none none\",\r\n        scrub: true, // 物件動畫根據卷軸捲動程度跑\r\n        start: \"top\",\r\n        // end: \"+=100\",\r\n        // markers: true,\r\n      },\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.title {\r\n  font-size: 5rem;\r\n  color: #b18d89;\r\n  background-color: rgb(63, 31, 28, 0.8);\r\n  /* 平均分給上下 */\r\n  line-height: 250px;\r\n  text-align: center;\r\n  height: 250px;\r\n  margin: 50px;\r\n}\r\n.title2 {\r\n  font-size: 5rem;\r\n  color: #b18d89;\r\n  background-color: rgb(63, 31, 28, 0.8);\r\n  /* 平均分給上下 */\r\n  line-height: 250px;\r\n  text-align: center;\r\n  height: 250px;\r\n  margin: 50px;\r\n}\r\n.btn {\r\n  border-color: transparent;\r\n  /* border: 0px solid; */\r\n}\r\n.btn-outline-light:hover {\r\n  color: #ffeb7b;\r\n  /* border-block-color: transparent; */\r\n  /* border: 0px; */\r\n  background-color: transparent;\r\n}\r\n.product {\r\n  background-color: rgba(240, 241, 243, 0.8);\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}