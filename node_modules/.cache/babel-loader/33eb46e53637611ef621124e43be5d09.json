AA,WANO,yBAMe;AAAA;;AAAA,UAAV,IAAU,uEAAH,CAAG;AACpB;AACA;AACA,UAAM,GAAE,aAAO,OAAO,CAAC,GAAR,CAAY,WAAnB,iBAAqC,OAAO,CAAC,GAAR,CAAY,YAAjD,mCAAsF,IAAtF,CAAR,CAHoB,CAIpB;;AACA,WAAK,SAAL,GAAiB,IAAjB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAmB,GAAnB,EANoB,CAQpB;;AACA,WAAK,KAAL,CAAW,GAAX,CAAe,GAAf,EAAoB,IAApB,CAAyB,UAAC,GAAD,EAAS;AAChC;AACA,QAAA,KAAI,CAAC,SAAL,GAAiB,KAAjB;;AACA,YAAI,GAAG,CAAC,IAAJ,CAAS,OAAb,EAAsB;AACpB,UAAA,OAAO,CAAC,GAAR,CAAY,KAAZ,EAAmB,GAAnB,EADoB,CAEpB;;AACA,UAAA,KAAI,CAAC,QAAL,GAAgB,GAAG,CAAC,IAAJ,CAAS,QAAzB;AACA,UAAA,KAAI,CAAC,UAAL,GAAkB,GAAG,CAAC,IAAJ,CAAS,UAA3B;AACF;AACD,OATD;AAUD,KAzBM;AA2BP;AACA;AACA,IAAA,SA7BO,qBA6BG,KA7BH,EA6BU,IA7BV,EA6BgB;AACrB;AACA,UAAI,KAAJ,EAAW;AACT;AACA,aAAK,WAAL,GAAmB,EAAnB;AACF,OAHA,MAGO;AACL;AACA;AACA,aAAK,WAAL,qBAAwB,IAAxB;AACF,OATqB,CAUrB;;;AACA,WAAK,KAAL,GAAa,KAAb,CAXqB,CAYrB;;AACA,UAAM,gBAAe,GAAI,KAAK,KAAL,CAAW,YAApC,CAbqB,CAcrB;;AACA,MAAA,gBAAgB,CAAC,SAAjB;AACD,KA7CM;AA8CP,IAAA,aA9CO,yBA8CO,IA9CP,EA8Ca;AAAA;;AAClB;AACA,WAAK,WAAL,GAAmB,IAAnB;AACA,UAAI,GAAE,GAAI,EAAV,CAHkB,CAGJ;;AACd,UAAI,UAAS,GAAI,EAAjB,CAJkB,CAIG;;AACrB,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,KAAjB;AACA,WAAK,SAAL,GAAiB,IAAjB;;AACA,UAAI,KAAK,KAAT,EAAgB;AACd;AACA;AACA;AACA,QAAA,GAAE,aAAO,OAAO,CAAC,GAAR,CAAY,WAAnB,iBAAqC,OAAO,CAAC,GAAR,CAAY,YAAjD,mBAAF;AACA,QAAA,UAAS,GAAI,MAAb;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;AACF,OAPA,MAOO;AACL;AACA;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;AACA,QAAA,GAAE,aAAO,OAAO,CAAC,GAAR,CAAY,WAAnB,iBAAqC,OAAO,CAAC,GAAR,CAAY,YAAjD,4BAA+E,IAAI,CAAC,EAApF,CAAF;AACA,QAAA,UAAS,GAAI,KAAb;AACF,OApBkB,CAqBlB;;;AACA,UAAM,gBAAe,GAAI,KAAK,KAAL,CAAW,YAApC,CAtBkB,CAuBlB;AACA;AACA;;AACA,WAAK,KAAL,CAAW,UAAX,EAAuB,GAAvB,EAA4B;AAAE,QAAA,IAAI,EAAE,KAAK;AAAb,OAA5B,EAAwD,IAAxD,CAA6D,UAAC,GAAD,EAAS;AACpE,QAAA,MAAI,CAAC,SAAL,GAAiB,KAAjB,CADoE,CAEpE;;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,GAAZ,EAHoE,CAIpE;;AACA,QAAA,gBAAgB,CAAC,SAAjB,GALoE,CAOpE;;AACA,YAAI,GAAG,CAAC,IAAJ,CAAS,OAAb,EAAsB;AACpB,UAAA,MAAI,CAAC,WAAL;;AACA,UAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,cAAlB,EAAkC;AAChC,YAAA,KAAK,EAAE,SADyB;AAEhC,YAAA,KAAK,EAAE;AAFyB,WAAlC;AAIF,SANA,MAMO;AACL,UAAA,MAAI,CAAC,OAAL,CAAa,IAAb,CAAkB,cAAlB,EAAkC;AAChC,YAAA,KAAK,EAAE,QADyB;AAEhC,YAAA,KAAK,EAAE,MAFyB;AAGhC,YAAA,OAAO,EAAE,GAAG,CAAC,IAAJ,CAAS,OAAT,CAAiB,IAAjB,CAAsB,GAAtB;AAHuB,WAAlC;AAKF,SApBoE,CAqBpE;AACA;AACA;AACA;;AACD,OAzBD;AA0BD,KAlGM;AAoGP;AACA,IAAA,mBArGO,+BAqGa,IArGb,EAqGmB;AACxB,WAAK,WAAL,qBAAwB,IAAxB;AACA,UAAM,YAAW,GAAI,KAAK,KAAL,CAAW,QAAhC;AACA,MAAA,YAAY,CAAC,SAAb;AACD,KAzGM;AA0GP;AACA;AACA,IAAA,UA5GO,wBA4GM;AAAA;;AACX,UAAM,GAAE,aAAO,OAAO,CAAC,GAAR,CAAY,WAAnB,iBAAqC,OAAO,CAAC,GAAR,CAAY,YAAjD,4BAA+E,KAAK,WAAL,CAAiB,EAAhG,CAAR;AACA,WAAK,SAAL,GAAiB,IAAjB;AACA,WAAK,KAAL,CAAW,MAAX,CAAkB,GAAlB,EAAuB,IAAvB,CAA4B,UAAC,QAAD,EAAc;AACxC,QAAA,MAAI,CAAC,SAAL,GAAiB,KAAjB;AACA,QAAA,OAAO,CAAC,GAAR,CAAY,QAAQ,CAAC,IAArB;AACA,YAAM,YAAW,GAAI,MAAI,CAAC,KAAL,CAAW,QAAhC;AACA,QAAA,YAAY,CAAC,SAAb;;AACA,QAAA,MAAI,CAAC,WAAL;AACD,OAND;AAOD;AAtHM,GAtBI;AA+Ib;AACA,EAAA,OAhJa,qBAgJH;AACR,SAAK,WAAL;AACD,GAlJY;AAmJb,EAAA,OAnJa,qBAmJH;AACR,IAAA,OAAO,CAAC,GAAR,CAAY,MAAZ,EAAoB,KAAK,KAAL,CAAW,YAA/B;AACD;AArJY,CAAf","sourcesContent":["<template>\r\n  <Loading :active=\"isLoading\"></Loading>\r\n  <div class=\"text-end\">\r\n    <!-- 抓ref.productModal 使用 showModal()-->\r\n    <!-- 因為把ProductModal元件抓來用了 所以可以使用showModal() -->\r\n    <!-- v-on:click=\"$refs.productModal\" -->\r\n    <!-- 傳送參數true = 點此按鈕時新增商品-->\r\n    <button v-on:click=\"openModal(true)\" class=\"btn btn-primary\" type=\"button\">\r\n      增加一個產品\r\n    </button>\r\n  </div>\r\n  <div>\r\n    <table class=\"table mt-4\">\r\n      <thead>\r\n        products.vue\r\n        <tr>\r\n          <th width=\"120\">分類</th>\r\n          <th>產品名稱</th>\r\n          <th width=\"120\">原價</th>\r\n          <th width=\"120\">售價</th>\r\n          <th width=\"100\">是否啟用</th>\r\n          <th width=\"200\">編輯</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <!-- 7. 渲染 -->\r\n        <tr v-for=\"item in products\" :key=\"item.id\">\r\n          <td>{{ item.category }}</td>\r\n          <td>{{ item.title }}</td>\r\n          <!-- 使用區域匯入方法 -->\r\n          <td class=\"text-right\">{{ currency(item.origin_price) }}</td>\r\n          <!-- 使用全域匯入方法 -->\r\n          <td class=\"text-right\">{{ $filters.currency(item.price) }}</td>\r\n          <td>\r\n            <!-- 8. 判斷是否啟用(1 or 0)，並套用各class -->\r\n            <span v-if=\"item.is_enabled\" class=\"text-success\">啟用</span>\r\n            <span v-else class=\"text-muted\">未啟用</span>\r\n          </td>\r\n          <td>\r\n            <!-- \"openModal(false,item) , item = v-for=\"item in products\"-->\r\n            <!-- false = 點此按鈕時非新增商品 = 編輯商品 -->\r\n            <div class=\"btn-group\">\r\n              <button\r\n                v-on:click=\"openModal(false, item)\"\r\n                class=\"btn btn-outline-primary btn-sm\"\r\n              >\r\n                編輯\r\n              </button>\r\n              <button\r\n                v-on:click=\"openDelProductModal(item)\"\r\n                class=\"btn btn-outline-danger btn-sm\"\r\n              >\r\n                刪除\r\n              </button>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n    <!-- 分頁元件 -->\r\n    <Pagination :pages=\"pagination\" @emit-pages=\"getProducts\"></Pagination>\r\n  </div>\r\n  <!-- 使用元件 ref似id 之後可以用this.$refs抓 -->\r\n  <!-- 製作傳送資料 v-bind props-->\r\n  <!-- 製作接受emit資料 v-on:update-product=\"updateProduct\"-->\r\n  <ProductModal\r\n    :product=\"tempProduct\"\r\n    v-on:update-product=\"updateProduct\"\r\n    ref=\"productModal\"\r\n  ></ProductModal>\r\n  <DelModal :item=\"tempProduct\" ref=\"delModal\" @del-item=\"delProduct\" />\r\n</template>\r\n\r\n<script>\r\n// 引用元件 元件內的 data methods mounted 都可以拿來用\r\nimport ProductModal from \"@/components/ProductModal.vue\";\r\nimport DelModal from \"@/components/DelModal.vue\";\r\n\r\n//匯入分頁元件\r\nimport Pagination from \"@/components/Pagination.vue\";\r\n\r\n//匯入分頁元件\r\nimport { currency } from \"../methods/filters.js\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      products: [],\r\n      // 1.分頁資訊\r\n      pagination: {},\r\n      // 新增產品倉庫\r\n      tempProduct: {},\r\n      // 判斷是否新增商品\r\n      isNew: false,\r\n      // 判斷是否要loading轉圈圈 倉庫\r\n      isLoading: false,\r\n    };\r\n  },\r\n  components: {\r\n    // 區域註冊元件\r\n    ProductModal,\r\n    DelModal,\r\n    Pagination,\r\n  },\r\n  // 引用emitter (因 Dashboard.vue provide )\r\n  inject: [\"emitter\"],\r\n  methods: {\r\n    // 千分號 方法引用\r\n    currency,\r\n\r\n    // 2.產品列表 (撈資料)\r\n    // page = 1 預設\r\n    getProducts(page = 1) {\r\n      // 3.使用API抓值 取得商品列表及分頁\r\n      // https://github.com/hexschool/vue3-course-api-wiki/wiki/%E7%AE%A1%E7%90%86%E6%8E%A7%E5%88%B6%E5%8F%B0-%5B%E9%9C%80%E9%A9%97%E8%AD%89%5D#%E5%8F%96%E5%BE%97%E5%95%86%E5%93%81%E5%88%97%E8%A1%A8\r\n      const api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/products/?page=${page}`;\r\n      // 撈到資料前有loding圈圈\r\n      this.isLoading = true;\r\n      console.log(\"api\", api);\r\n\r\n      // 4.axios get(api, 欲傳送的值)方法\r\n      this.$http.get(api).then((res) => {\r\n        // 撈到資料loding圈圈關閉\r\n        this.isLoading = false;\r\n        if (res.data.success) {\r\n          console.log(\"res\", res);\r\n          // 6. 把 產品資訊 products 與 分頁資訊 pagination 存起來\r\n          this.products = res.data.products;\r\n          this.pagination = res.data.pagination;\r\n        }\r\n      });\r\n    },\r\n\r\n    // 按鈕跳窗方法\r\n    // (是否新增, 帶入產品資料)\r\n    openModal(isNew, item) {\r\n      // if isNew = true\r\n      if (isNew) {\r\n        // 新增商品\r\n        this.tempProduct = {};\r\n      } else {\r\n        // 編輯商品\r\n        // ... 展開舊的item\r\n        this.tempProduct = { ...item };\r\n      }\r\n      // 儲存狀態\r\n      this.isNew = isNew;\r\n      // this.$refs.productModal = html 內 <ProductModal ref=\"productModal\"></ProductModal>\r\n      const productComponent = this.$refs.productModal;\r\n      // 顯示<ProductModal ref=\"productModal\"></ProductModal> (方法在ProductModal.vue)\r\n      productComponent.showModal();\r\n    },\r\n    updateProduct(item) {\r\n      // 從內層(ProductModal.vue)接收資料, 送進tempProduct倉庫\r\n      this.tempProduct = item;\r\n      let api = \"\"; //API初始化\r\n      let httpMethod = \"\"; //api (get,post,put,delect)方法初始化\r\n      console.log(this.isNew);\r\n      this.isLoading = true;\r\n      if (this.isNew) {\r\n        // 新增\r\n        // API使用方式 取得商品列表\r\n        // https://github.com/hexschool/vue3-course-api-wiki/wiki/%E7%AE%A1%E7%90%86%E6%8E%A7%E5%88%B6%E5%8F%B0-%5B%E9%9C%80%E9%A9%97%E8%AD%89%5D#%E5%95%86%E5%93%81%E5%BB%BA%E7%AB%8B\r\n        api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product`;\r\n        httpMethod = \"post\";\r\n        console.log(\"新增\");\r\n      } else {\r\n        // 編輯\r\n        // https://github.com/hexschool/vue3-course-api-wiki/wiki/%E7%AE%A1%E7%90%86%E6%8E%A7%E5%88%B6%E5%8F%B0-%5B%E9%9C%80%E9%A9%97%E8%AD%89%5D#%E4%BF%AE%E6%94%B9%E7%94%A2%E5%93%81\r\n        console.log(\"修改\");\r\n        api = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product/${item.id}`;\r\n        httpMethod = \"put\";\r\n      }\r\n      // this.$refs.productModal = html 內 <ProductModal ref=\"productModal\"></ProductModal>\r\n      const productComponent = this.$refs.productModal;\r\n      // axios post(api, 欲傳送的值)方法方法\r\n      // title(String)、category(String)、unit(String)、origin_price(Number)、price(Number) 為必填欄位\r\n      // [動態，隨時抽換]\r\n      this.$http[httpMethod](api, { data: this.tempProduct }).then((res) => {\r\n        this.isLoading = false;\r\n        // then 成功\r\n        console.log(res);\r\n        // 執行 ProductModal.vue 的 hideModal()\r\n        productComponent.hideModal();\r\n\r\n        // 錯誤訊息(吐司樣式) 判斷式\r\n        if (res.data.success) {\r\n          this.getProducts();\r\n          this.emitter.emit(\"push-message\", {\r\n            style: \"success\",\r\n            title: \"更新成功\",\r\n          });\r\n        } else {\r\n          this.emitter.emit(\"push-message\", {\r\n            style: \"danger\",\r\n            title: \"更新失敗\",\r\n            content: res.data.message.join(\"、\"),\r\n          });\r\n        }\r\n        // 但 吐司錯誤訊息程式碼 會因為點擊越來越多，故拆開 製作成另一方法\r\n        // 因此 methods > pushMessageState.js \r\n        // 並 全域匯入 給 Coupons.vue & Orders.vue 用\r\n        // 邏輯：因為直接用全域只會有全域一個倉庫(提升效能)，但區域就會到處都有倉庫\r\n      });\r\n    },\r\n\r\n    // 開啟刪除 Modal\r\n    openDelProductModal(item) {\r\n      this.tempProduct = { ...item };\r\n      const delComponent = this.$refs.delModal;\r\n      delComponent.showModal();\r\n    },\r\n    // API 刪除產品\r\n    // https://github.com/hexschool/vue3-course-api-wiki/wiki/%E7%AE%A1%E7%90%86%E6%8E%A7%E5%88%B6%E5%8F%B0-%5B%E9%9C%80%E9%A9%97%E8%AD%89%5D#%E5%88%AA%E9%99%A4%E7%94%A2%E5%93%81\r\n    delProduct() {\r\n      const url = `${process.env.VUE_APP_API}api/${process.env.VUE_APP_PATH}/admin/product/${this.tempProduct.id}`;\r\n      this.isLoading = true;\r\n      this.$http.delete(url).then((response) => {\r\n        this.isLoading = false;\r\n        console.log(response.data);\r\n        const delComponent = this.$refs.delModal;\r\n        delComponent.hideModal();\r\n        this.getProducts();\r\n      });\r\n    },\r\n  },\r\n\r\n  //  5.生命週期 馬上執行\r\n  created() {\r\n    this.getProducts();\r\n  },\r\n  mounted() {\r\n    console.log(\"refs\", this.$refs.productModal);\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}