utton type=\"button\" class=\"btn btn-outline-secondary\" data-bs-dismiss=\"modal\">取消</button>\r\n          <button type=\"button\" class=\"btn btn-danger\" @click=\"$emit('del-item')\">確認刪除</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n// @ = src\r\nimport modalMixin from \"@/mixins/modalMixin\";\r\n\r\nexport default {\r\n  props: {\r\n    item: {},\r\n  },\r\n  data() {\r\n    return {\r\n      modal: \"\",\r\n    };\r\n  },\r\n  mixins: [modalMixin],\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}